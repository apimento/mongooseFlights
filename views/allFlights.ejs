<h1>Flights</h1> 

<table table class="table table-striped"> 
    <tr>
        <th>Airline</th> 
        <th>Airport</th>
        <th>FlightNo</th> 
        <th>Departs</th>   
        <th> </th>
        <th> </th>
    </tr> 

    <% flights.forEach(function(flight) { %>
    <tr>
        <td> <%=flight.airline %> </td>
        <td> <%=flight.airport %> </td> 
        <td> <%=flight.flightNumber %> </td>
        <td> <%=flight.departs %> </td>     

        <td><a href="/flightDetails?id=<%=flight.id%>"> <button type="submit">See Details</button></a></td>
        
    </tr>

    <% }) %>
</table>